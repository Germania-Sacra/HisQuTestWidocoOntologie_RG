    <!-- http://webprotege.stanford.edu/Taxae -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Taxae">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Fundstelle_Typ"/>
        <webprotege:bezieht_sich_auf>RG IV</webprotege:bezieht_sich_auf>
