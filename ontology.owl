        <webprotege:bezieht_sich_auf>RG X</webprotege:bezieht_sich_auf>
        <webprotege:stringInRG xml:lang="la">S</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Supplikenregister</rdfs:label>
        <skos:altLabel xml:lang="la">Archivio Segreto Vaticano, Registra Supplicationum</skos:altLabel>
        <skos:altLabel>Archivio Segreto Vaticano, Registri delle Suppliche</skos:altLabel>
        <skos:altLabel>Reg. Suppl. 44-76</skos:altLabel>
