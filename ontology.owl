    <owl:Class rdf:about="http://webprotege.stanford.edu/Dukat">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Waehrung_Typ"/>
        <webprotege:stringInRG xml:lang="la">adc.</webprotege:stringInRG>
