        <webprotege:stringInRG xml:lang="la">translatio</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Übertragung</rdfs:label>
        <skos:editorialNote xml:lang="de">Bezieht sich auf Objekte: 
- Rentenzahlung
- Patronatsrecht
- Klosterübertritt
- Präsentationsrecht
- Taufbecken
- Friedhof

Bezieht sich auf Gnaden:
- Ablass

Bezieht sich auf Institutionen (?):
- Stiftskirche</skos:editorialNote>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Umstritten -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Umstritten">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Pfruenden_Status_Besetzung"/>
