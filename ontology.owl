        <rdfs:label xml:lang="de">Suffragandiözese</rdfs:label>
        <skos:definition xml:lang="la">Eine Diözese, die einer Erzdiözese untergeordnet ist.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Supplik -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Supplik">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Objekt_Typ"/>
        <rdfs:label xml:lang="de">Supplik</rdfs:label>
        <rdfs:label xml:lang="la">supplicatio</rdfs:label>
        <skos:altLabel xml:lang="la">petitio</skos:altLabel>
        <skos:definition xml:lang="de">Das Schriftstück, das an der Kurie von Petenten eingereicht wird, um einen Gnadenerweis/Provisio etc. in Form einer Littera zu erwirken. 

In seltenen Fällen erfolgt die Genehmigung des Gesuchs durch eine Unterschrift direkt auf der Supplik selbst, ohne zusätzliche Ausstellung einer Littera, vgl. Expeditionsmodalität.</skos:definition>
    </owl:Class>
    


