        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Entsendung"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <rdfs:label xml:lang="de">P11 had participant Entsandter (participated in as Entsandter)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz der Activity Entsendung mit der Person, die entsandt wurde.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Fuersprecher_(participated_in_as_Fuersprecher) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Fuersprecher_(participated_in_as_Fuersprecher)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Activity_(E7)"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <webprotege:stringInRG xml:lang="la">supplic.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">supplicante</webprotege:stringInRG>
        <rdfs:label xml:lang="de">P11 had participant Fürsprecher (participated in as Fürsprecher)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Activity mit der Person, die an der Kurie für einen Petenten Fürsprache einlegt. Der Fürsprecher muss nicht selbst an der Kurie anwesend sein. I.d.R. eine prominente Person.</skos:definition>
        <skos:example xml:lang="la">Albertus Holteker cler. Magunt. de (Johanne de Feys datario pro fam. supplic. ) benef. Avinion. 21 apr. 1420 S 141 121v.</skos:example>
        <skos:example xml:lang="la">RG IX 00026: Admont Abb. et conv. mon. A. o.s. Ben. Salzeburg. dioc. : supplic. Friderico R.I. de indulg. 5 an. pro par. eccl. b. Marie Cholben Salzeburg. dioc. d. mon. incorp.</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Kommissionsempfaenger_(participated_in_as_Kommissionsempfaenger) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Kommissionsempfaenger_(participated_in_as_Kommissionsempfaenger)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Kommissionsvermerk"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <rdfs:label xml:lang="de">P11 had participant Kommissionsempfänger (participated in as Kommissionsempfänger)</rdfs:label>
        <skos:editorialNote xml:lang="de">Verbindet einen Kommissionsvermerk mit der Person, die nach dem Vermerk committ. genannt wird.</skos:editorialNote>
        <skos:example xml:lang="la">RG V 00061: Agnes de Bronchorst (Brouchorst) de illustri gen., can. sec. eccl. [s. Viti] Altinen. (de Elten) Traiect. dioc., in 22 an. sue et. constit., in abbam. d. eccl. el. de disp. sup. def. et. ( committ. mag. Petro de Mera prep. eccl. Embricen. [Traiect. dioc.]) 17. apr. 1443 S 391 146v.</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Obligationsbeauftragter_(participated_in_as_Obligationsbeauftragter) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Obligationsbeauftragter_(participated_in_as_Obligationsbeauftragter)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Activity_(E7)"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <rdfs:label xml:lang="de">P11 had participant Obligationsbeauftragter (participated in as Obligationsbeauftragter)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz von Activity mit einer Person, die stellvertretend für eine andere Person die Zahlungsverpflichtung für den Empfang einer Pfründe an der Kurie abgibt; eingeleitet durch oblig. p. m.</skos:definition>
        <skos:example xml:lang="la">RG 5/40: o.s.a (obligatio super annata) per Johannem Scade procuratorem causarum in Romana curia</skos:example>
        <skos:example xml:lang="la">RG II 00012: Anselmus abb. mon. s. Petri Novellarien [o. s. Ben.] Argent. dioc. oblig. p. m. Johannis Kesseler dec. eccl. s. Jacobi Bamberg. et recogn. oblig. pro Johanne predec. 23 iul. 1388 O 48 106v.</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Petent_(participated_in_as_Petent) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Petent_(participated_in_as_Petent)">
