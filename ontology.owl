        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Activity_(E7)"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <rdfs:label xml:lang="de">P11 had participant Weihempfänger (participated in as Weihempfänger)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz von Activity, mit einer Person, die eine Weihe empfangen hat.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Zahlungsbeauftragter_(participated_in_as_Zahlungsbeauftragter) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Zahlungsbeauftragter_(participated_in_as_Zahlungsbeauftragter)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Geldzahlung"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <webprotege:stringInRG xml:lang="la">solv. p.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">solv. p. m.</webprotege:stringInRG>
        <rdfs:label xml:lang="de">P11 had participant Zahlungsbeauftragter (participated in as Zahlungsbeauftragter)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz der Activity Geldzahlung mit einer Person, die für eine andere Person die Ausführung der Geldzahlung übernommen hat.</skos:definition>
        <skos:editorialNote xml:lang="de">Ein Zahlungsbeauftragter ist eine Person, die vom Zahler (meist der Petent) mit der Begleichung von Geldschulden an der Kurie beauftragt wurde. Im RG eingeleitet durch &quot;solv. p. m. + Personenname&quot; o. Ä.</skos:editorialNote>
        <skos:example xml:lang="la">RG III 00213: […] solv. p. m. Georgii Vredelant 20 fl. […]</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Zahlungsempfaenger_(participated_in_as_Zahlungsempfaenger) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Zahlungsempfaenger_(participated_in_as_Zahlungsempfaenger)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Geldzahlung"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Actor_(E39)"/>
        <rdfs:label xml:lang="de">P11 had participant Zahlungsempfänger (participated in as Zahlungsempfänger)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz der Activity Geldzahlung mit einem Actor, der das Geld erhält.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Zeuge_(participated_in_as_Zeuge) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Zeuge_(participated_in_as_Zeuge)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_(participated_in)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Event_(E5)"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <rdfs:label xml:lang="de">P11 had participant Zeuge (participated in as Zeuge)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz von Event mit einer Person, die als Zeuge agiert.</skos:definition>
        <skos:editorialNote xml:lang="de">Zeugen können in Gerichtsprozessen auftreten, aber auch im Kontext von Zahlungen und Obligationen, siehe Beispiele. Manchmal ist der Kontext auch unbestimmt. Deshalb ist diese Property keine Subproperty von Prozessbeteiligter.</skos:editorialNote>
        <skos:example xml:lang="la">Adolphus filius ducis Cleven. testis 21. febr. 1440 DC 20 141r.</skos:example>
        <skos:example xml:lang="la">Jacobus Nedermolen cler. Colon. dioc. testis super quadam oblig. 28 iul. 1409 M 8.</skos:example>
        <skos:example xml:lang="la">Johannes de Bulgaro cler. Colon. dioc. testis super quadam solut. 8 oct. 1409 M 11v.</skos:example>
    </owl:ObjectProperty>
    


