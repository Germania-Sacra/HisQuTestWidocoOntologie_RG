    <!-- http://webprotege.stanford.edu/motu_proprio -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/motu_proprio">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Expeditionsmodalitaet"/>
        <webprotege:stringInRG xml:lang="la">motu pr.</webprotege:stringInRG>
