        <webprotege:bezieht_sich_auf>RG X</webprotege:bezieht_sich_auf>
        <webprotege:stringInRG>Acquisti 27/1</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Acquisti_27/1_(Preziosi)</rdfs:label>
        <skos:altLabel>Archivio di Stato di Roma</skos:altLabel>
        <skos:definition xml:lang="de">RG X unter Brevenregister;
