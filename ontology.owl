    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Fundstelle -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Fundstelle">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="de">Fundstelle</rdfs:label>
        <skos:altLabel xml:lang="de">Quelle</skos:altLabel>
        <skos:definition>Fundstellen sind die Registerserien, aus denen sich die Inhalte der Regesten speisen und die in den RG-Bänden abgekürzt angegeben werden.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Fundstelle_Typ -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Fundstelle_Typ">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Typ_(E55)"/>
        <rdfs:label xml:lang="de">Fundstelle_Typ</rdfs:label>
        <skos:definition xml:lang="de">Hier sind nur die häufigsten Fundstellen aufgelistet. Siehe dazu einen Satz aus dem RG II: &quot;Andere selten zitierte Quellen werden mit Archiv- oder Bibliothekssignaturen zitiert, die ohne weiteres verständlich sind.&quot;</skos:definition>
    </owl:Class>
    


