    <!-- http://webprotege.stanford.edu/P11_had_participant_Prozessvorsteher_(participated_in_as_Prozessvorsteher) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Prozessvorsteher_(participated_in_as_Prozessvorsteher)">
        <rdfs:subPropertyOf rdf:resource="http://webprotege.stanford.edu/P11_had_participant_Prozessbeteiligter_(participated_in_as_Prozessbeteiligter)"/>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/Gerichtsprozess"/>
        <rdfs:range rdf:resource="http://webprotege.stanford.edu/Person_(E21)"/>
        <webprotege:stringInRG xml:lang="la">iudex</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">officialis</webprotege:stringInRG>
        <rdfs:label xml:lang="de">P11 had participant Prozessvorsteher (participated in as Prozessvorsteher)</rdfs:label>
        <skos:definition xml:lang="de">Verbindet eine Instanz der Activity Gerichtsprozess mit einer Person, die in einem Gerichtsprozess als Richter auftritt.</skos:definition>
        <skos:editorialNote xml:lang="de">Die sperrige Bezeichnung &quot;Prozessvorsteher&quot; (statt eingängiger &quot;Richter&quot;) ist dem Umstand geschuldet, dass nicht nur amtliche &quot;Richter&quot; (vgl. WeltlichesAmt) als Richter auftreten, sondern auch der &quot;Offizial&quot;, &quot;Delegierter Richter&quot; o.ä.

Der Prozessvorsteher wird oft über ein Präpositionalobjekt mit &quot;coram + Name des Richters&quot; angegeben.</skos:editorialNote>
        <skos:example xml:lang="la">RG IV 00007: […] sup. quib. litig. contra Johannem de Hamelburgh presb. coram Johanne Sindra officiali Patav. 29 apr. 1423 L 235 105v.</skos:example>
        <skos:example xml:lang="la">RG V 01395: [...] iudex in lite sup. benef. Leod. dioc. […]</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://webprotege.stanford.edu/P11_had_participant_Rechtsbeistand_(participated_in_as_Rechtsbeistand) -->


    <owl:ObjectProperty rdf:about="http://webprotege.stanford.edu/P11_had_participant_Rechtsbeistand_(participated_in_as_Rechtsbeistand)">
