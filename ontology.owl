    <!-- http://webprotege.stanford.edu/Kassation -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kassation">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Gnadenerweis_Typ"/>
        <webprotege:stringInRG xml:lang="la">cass.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">cassatio</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kassation</rdfs:label>
        <skos:definition xml:lang="de">Cassatio (von lateinisch cassare = „für nichtig erklären“, „aufheben“) bezeichnet im kirchengerichtlichen Verfahren die Aufhebung eines Urteils durch ein höheres kirchliches Gericht.</skos:definition>
        <skos:editorialNote xml:lang="de">Bezug zu Objekten: 
- Güterentfremdung (alienatio bonorum)
- Gewohnheit (consuetudo)
- Wahl (electio)
- Interdikt
- Littera
- Mandat
- Pfründentausch (permutatio)
- Statut
- Urteil (sententia)
- Prozess (processus)
- Pension
- Bestimmung (constitutio)

Bezug zu anderen Gnadenerweisen: 
- Exemption
- Bestätigung
- Inkorporation
- Unklare_Gnade
- Absolution
- Bestätigung (confirmatio)
- einfache Provisio</skos:editorialNote>
        <skos:example xml:lang="la">RG II 02980: […] lic. cass. quaedam eccl. mai. statuta […]</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kathedrale -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kathedrale">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirche"/>
        <webprotege:stringInRG xml:lang="la">cathedr. eccl.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">eccl. cathedralis</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">ecclesia cathedralis</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kathedrale</rdfs:label>
        <skos:definition xml:lang="de">Eine Kathedrale ist die Hauptkirche eines Bistums und Sitz des Bischofs. Das Wort „Kathedrale“ leitet sich vom griechischen kathedra ab, was „Sitz“ bedeutet (gemeint ist der Bischofssitz).</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kauf -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kauf">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Objekt_Typ"/>
        <webprotege:stringInRG xml:lang="la">emptio</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kauf</rdfs:label>
        <skos:example xml:lang="la">RG III 00638: Esszlingen hosp. paup. situm in op. Constant. dioc.: conf. emptionem pratorum ac al. bonorum in villa Diczisow Constant. dioc. ac donationem iuris patron. super par. eccl. in dicta villa ex possess. Eberhardi Burgermaister incole dicti op. factam 22 oct. 1411 L 157 113.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kellner_Kloster -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kellner_Kloster">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Klosteramt_Amt"/>
        <webprotege:stringInRG xml:lang="la">cellerarius</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kellner_Kloster</rdfs:label>
        <skos:altLabel xml:lang="de">Kellerer</skos:altLabel>
        <skos:definition xml:lang="de">&quot;[…] der die Beaufsichtigung bei der Verteilung der Vorräte durchzuführen hatte&quot; (Wentz/Schwineköper, 1,1 S. 134)</skos:definition>
        <skos:editorialNote xml:lang="de">vgl. Kellner_Stift</skos:editorialNote>
        <skos:example xml:lang="la">RG V 08341: Simon cellerarius et can. mon. Nuemburg. o. s. Aug. Patav. dioc. lic. elig. confess. 28. apr. 1436 L 346 276v.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kellner_Stift -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kellner_Stift">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Stiftsamt_Amt"/>
        <rdfs:label xml:lang="de">Kellner_Stift</rdfs:label>
        <rdfs:label xml:lang="la">cellerarius</rdfs:label>
        <skos:altLabel xml:lang="de">Kellerer</skos:altLabel>
        <skos:definition xml:lang="de">&quot;[…] der die Beaufsichtigung bei der Verteilung der Vorräte durchzuführen hatte&quot; (Wentz/Schwineköper, 1,1 S. 134)</skos:definition>
        <skos:editorialNote xml:lang="de">vgl. Kellner_Kloster</skos:editorialNote>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirche -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirche">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirchliche_Institution_Typ"/>
        <webprotege:stringInRG xml:lang="la">eccl.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">ecclesia</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kirche</rdfs:label>
        <skos:definition xml:lang="de">Eine kirchliche Organisationseinheit, die mit einem Gebäude zusammenfällt. Sie ist meist mit Personal und Pfründen ausgestattet. Sie hat Rechte und Pflichten, v. a. in der Seelsorge der Gläubigen.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirche_Gebaeude -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirche_Gebaeude">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Gebaeude_Gebaeudeteil"/>
        <rdfs:label xml:lang="de">Kirche_Gebäude</rdfs:label>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchenfabrik_Vorsteher -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchenfabrik_Vorsteher">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirchliches_Amt_Typ"/>
        <webprotege:stringInRG xml:lang="la">mag. fabrice</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kirchenfabrik_Vorsteher</rdfs:label>
        <rdfs:label xml:lang="la">magister fabrice</rdfs:label>
        <skos:example xml:lang="la">RG IV 07678: ... custos et mag. fabrice eccl. August.: indulg. pro eccl. August. 25 aug. 1428 L 283 159.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchenprovinz -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchenprovinz">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirchliche_Institution_Typ"/>
        <webprotege:stringInRG xml:lang="la">provincia</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kirchenprovinz</rdfs:label>
        <skos:definition xml:lang="de">Eine Kirchenprovinz ist ein Zusammenschluss mehrerer benachbarter Diözesen unter der Leitung eines Erzbischofs (Metropoliten), dessen Erzdiözese das kirchliche Zentrum bildet. Sie bildet die größte kirchliche Verwaltungseinheit.</skos:definition>
        <skos:example xml:lang="la">RG IV 03111: Fredericus de Parsberg dec. Ratisbon.: constit. nuntius et collector generalis in provincia Salzeburg. excepta parte Patav. dioc. in qua plene Alberto duci Austrie obeditur 1 dec. 1427 V 351 31.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchliche_Institution -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchliche_Institution">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Institution"/>
        <rdfs:label xml:lang="de">Kirchliche_Institution</rdfs:label>
        <skos:definition xml:lang="de">Eine kirchliche Institution ist eine Institution innerhalb der christlichen Kirche, die einem geistlichen Zweck dient.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchliche_Institution_Typ -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchliche_Institution_Typ">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Typ_(E55)"/>
        <rdfs:label xml:lang="de">Kirchliche_Institution_Typ</rdfs:label>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchlicher_Stand -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchlicher_Stand">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="de">Kirchlicher_Stand</rdfs:label>
        <skos:editorialNote xml:lang="de">Kann nicht als Typ über P2 modelliert werden, wenn wir Übergange einer Person vom Laien- in den Klerikerstand modellieren wollen.</skos:editorialNote>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchliches_Amt -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchliches_Amt">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Amt"/>
        <rdfs:label xml:lang="de">Kirchliches_Amt</rdfs:label>
        <skos:definition xml:lang="de">Ein kirchliches Amt ist eine offizielle Stellung oder Funktion, die eine Person innerhalb der römisch-katholischen Kirche einnahm. Ein kirchliches Amt kann als Ehrenamt (z. B. Senior in einem Kollegiatsstift) ausgeübt werden oder mit Einnahmen verbunden sein (z. B. Propst in einem Kollegiatsstift).</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kirchliches_Amt_Typ -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kirchliches_Amt_Typ">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Typ_(E55)"/>
        <rdfs:label xml:lang="de">Kirchliches_Amt_Typ</rdfs:label>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klarissen -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klarissen">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Orden"/>
        <webprotege:stringInRG xml:lang="la">o. s. Clare</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Klarissen</rdfs:label>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kleriker -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kleriker">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirchlicher_Stand"/>
        <rdfs:label xml:lang="de">Kleriker</rdfs:label>
        <skos:definition xml:lang="de">Eine Person, die eine Weihe empfangen hat und alle Personen, die mit einem kirchlichen Amt oder mit der Bezeichnung clericus aufgeführt werden.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kloster -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kloster">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirchliche_Institution_Typ"/>
        <webprotege:stringInRG xml:lang="la">mon.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">monasterium</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kloster</rdfs:label>
        <skos:definition xml:lang="de">Ein Kloster ist eine religiöse Gemeinschaft, in der Mönche oder Nonnen nach festen Regeln zusammenleben.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kloster_Gebaeudekomplex -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kloster_Gebaeudekomplex">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Gebaeude_Gebaeudeteil"/>
        <rdfs:label xml:lang="de">Kloster_Gebäudekomplex</rdfs:label>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosteramt -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosteramt">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirchliches_Amt_Typ"/>
        <rdfs:label xml:lang="de">Klosteramt</rdfs:label>
        <skos:definition xml:lang="de">Kirchliche Ämter, die innerhalb eines Klosters bekleidet werden konnten.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosteramt_Amt -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosteramt_Amt">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Klosteramt"/>
        <rdfs:label xml:lang="de">Klosteramt_Amt</rdfs:label>
        <skos:definition xml:lang="de">Klosterämter, die keine Leitungsaufgaben beinhalteten.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosteramt_Leitung -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosteramt_Leitung">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Klosteramt"/>
        <rdfs:label xml:lang="de">Klosteramt_Leitung</rdfs:label>
        <skos:definition xml:lang="de">Klosterämter, die Leitungsaufgaben beinhalteten.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klostereintritt -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klostereintritt">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Objekt_Typ"/>
        <webprotege:stringInRG xml:lang="la">mon. ingredi</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">ord. ingredi</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Klostereintritt</rdfs:label>
        <skos:example xml:lang="la">RG I 00021: […] lic. ingrediendi in mon. de Mazmon. Basil. dioc. […]</skos:example>
        <skos:example xml:lang="la">RG V 00313: Andreas Remberti Andreas Remberti prof. o. fr. herem. s. Aug., domum Osnaburg. d. ord. ingressus lic. se transf. ad aliquod mon. sub capit. mon. de Windesim Traiect. dioc. ord. can. reg. de stricta observ. 7. ian. 1439 L 362 287rs.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosterflucht -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosterflucht">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Objekt_Typ"/>
        <webprotege:stringInRG xml:lang="la">apostasia</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">mon. illicite exivit</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Klosterflucht</rdfs:label>
        <skos:example xml:lang="la">RG IV 00069: […] de absol. quod olim de mon. in Clingental o. s. Aug. Basil. et postea de mon. in Riintal o. Cist. illicite exivit […]</skos:example>
        <skos:example xml:lang="la">RG IX 00294: […] ab apostasie reatu et excom. sent. absol. […]</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klostergemeinschaft -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klostergemeinschaft">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Personengruppe_(E74)"/>
        <webprotege:stringInRG xml:lang="la">abb. etc.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">prep. etc. mon.</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Klostergemeinschaft</rdfs:label>
        <skos:definition xml:lang="de">Die Klostergemeinschaft kann als Personengruppe im RG auftreten: abbas bzw. prepositus et conventus monasterii ...</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klostergruendung -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klostergruendung">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Gruendung"/>
        <rdfs:label xml:lang="de">Klostergründung</rdfs:label>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosterkapelle -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosterkapelle">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kapelle"/>
        <rdfs:label xml:lang="de">Klosterkapelle</rdfs:label>
        <skos:definition xml:lang="de">Eine Kapelle, die sich in einem Kloster befindet.</skos:definition>
        <skos:example xml:lang="la">RG IV 00778: Balduinus (de Wenden) prior mon. s. Michaelis Lunenborg. o. s. Ben. Verden. dioc. presb. decr. doct. de indulg. pro eccl. et capella b. Catherine virg. 29 dec. 1429 S 251 48v, L 295 10v.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosterkirche -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosterkirche">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kirche"/>
        <rdfs:label xml:lang="de">Klosterkirche</rdfs:label>
        <skos:definition xml:lang="de">Eine Klosterkirche ist die zentrale Kirche einer klösterlichen Gemeinschaft. Sie dient hauptsächlich dem Stundengebet, der Messfeier und der klösterlichen Liturgie.</skos:definition>
        <skos:example xml:lang="la">RG VI 00017: Adam Tragas presb. de nova prov. de alt. s. Margarete in eccl. mon. Dalen o.Cist. e.m. Magunt. (24 m.arg. ), ad quod vac. p.o. Johannis Rumelfels present. fuit p. patron., n.o. vicar. in eccl. b. Marie ad grad. Magunt. (4) 19. nov. 1450 S 446 250rs.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosterreform -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosterreform">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Objekt_Typ"/>
        <webprotege:stringInRG xml:lang="la">visitandi et reformandi mon.</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Klosterreform</rdfs:label>
        <skos:example xml:lang="la">RG II 01505: […] m. reformandi et visitandi nonnulla mon. in Almania […]</skos:example>
        <skos:example xml:lang="la">RG IX 00033:[…] de fac. visitandi et reformandi mon. […]</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosterterritorium -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosterterritorium">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Weltliche_Institution_Typ"/>
        <rdfs:label xml:lang="de">Klosterterritorium</rdfs:label>
        <skos:definition xml:lang="de">Weltliche Besitzung eines Klosters</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Klosteruebertritt -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Klosteruebertritt">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Objekt_Typ"/>
        <webprotege:stringInRG xml:lang="la">ad mon. transferre</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">transire ad al. mon.</webprotege:stringInRG>
