        <skos:definition xml:lang="de">Ein Bischof, der gleichzeitig die Kardinalswürde innehat.</skos:definition>
        <skos:example xml:lang="la">RG II 00305: Alardus Johannis Alardi de Molendino script. abbrev. fam. cap. Philippi Ostien. ep. card. prov. de can. sub expect. preb. Warm., n. o. can. et preb. Traiect., lit. sup. can. et preb. mai. eccl. s. Sebastiani Magdeburg. dim., can. et preb. mai. Paderburn. dim., can. sub expect. preb. Wratislav., dign. in civ. vel. dioc. Traiect. vacat., benef. ad coll. ep. etc. Leod. dim. quoad cur., 9 nov. 1389 L 3 188.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kardinaldiakon -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kardinaldiakon">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Kardinal"/>
        <webprotege:stringInRG xml:lang="la">diac. card.</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kardinaldiakon</rdfs:label>
        <skos:definition xml:lang="de">Ein Diakon, der gleichzeitig die Kardinalswürde innehat.</skos:definition>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kardinalpriester -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kardinalpriester">
