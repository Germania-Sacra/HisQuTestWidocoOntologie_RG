        <webprotege:stringInRG xml:lang="la">presb. card.</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Kardinalpriester</rdfs:label>
        <skos:definition xml:lang="de">Ein Priester, der gleichzeitig die Kardinalswürde innehat.</skos:definition>
        <skos:example xml:lang="la">RG VII 00266: Bertoldus Nathyeyse al. Castede cler. Magunt. dioc. Johannis tit. s. Clementis presb. card. fam. nullum benef. obtin. de prerog. fam. ap. quoad assec. benef. 14 mart. 1458 S 507 174r.</skos:example>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Karmeliter -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Karmeliter">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Orden"/>
        <webprotege:stringInRG xml:lang="la">b. Marie Carmel</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">o. Carm.</webprotege:stringInRG>
        <rdfs:label xml:lang="de">Karmeliter</rdfs:label>
        <skos:altLabel xml:lang="de">Karmeliten</skos:altLabel>
    </owl:Class>
    


    <!-- http://webprotege.stanford.edu/Kartaeuser -->


    <owl:Class rdf:about="http://webprotege.stanford.edu/Kartaeuser">
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/Orden"/>
        <webprotege:stringInRG xml:lang="la">Carthus ord.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">Carthus. o.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">Carthusiensibus</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">conventus Carthus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">dom. Carthus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">dom. Cartus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">dom. Cartusien</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">domum Carthus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">domum Carthusien.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">domum Cartus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">domum Cartusien.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">domus Cartusien.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">fr. Cart.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">fr. Carthus</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">mon. Carthus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">mon. Cartus.</webprotege:stringInRG>
        <webprotege:stringInRG xml:lang="la">o. C. et. Cart.</webprotege:stringInRG>
